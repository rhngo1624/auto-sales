<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>


<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.objectmanip.AddAccessoryController">
   <children>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #2d2e30;">
         <items>
            <JFXButton fx:id="closeButton" onAction="#close" ripplerFill="WHITE" text="Close" textFill="WHITE" />
         </items>
      </ToolBar>
      <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="378.0" prefWidth="564.0" style="-fx-background-color: #202630;">
         <children>
            <VBox prefHeight="400.0" prefWidth="237.0" spacing="30.0">
               <children>
                  <JFXTextField fx:id="nameField" focusColor="#00ff90" labelFloat="true" maxWidth="263.0" minWidth="263.0" prefHeight="33.0" prefWidth="263.0" promptText="Name" unFocusColor="WHITE">
                     <VBox.margin>
                        <Insets top="45.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <JFXTextArea fx:id="descriptionField" focusColor="#00ff90" labelFloat="true" maxWidth="380.0" minWidth="261.0" prefHeight="125.0" prefWidth="263.0" promptText="Description" unFocusColor="WHITE">
                     <VBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </VBox.margin>
                  </JFXTextArea>
                  <JFXTextField fx:id="priceField" focusColor="#00ff90" labelFloat="true" maxWidth="263.0" minWidth="263.0" prefHeight="33.0" prefWidth="263.0" promptText="Price" unFocusColor="WHITE" />
                  <JFXButton fx:id="addButton" onAction="#add" buttonType="RAISED" prefHeight="27.0" prefWidth="304.0" ripplerFill="#00ff90" style="-fx-background-color: #23272d;" text="Add" textFill="WHITE">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Futura Bold" size="13.0" />
                     </font>
                  </JFXButton>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="308.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="205.0" fitWidth="324.0" opacity="0.79" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imgs/icons/delete.png" />
                     </image>
                     <VBox.margin>
                        <Insets top="60.0" />
                     </VBox.margin>
                  </ImageView>
                  <JFXButton fx:id="uploadButton" onAction="#upload" buttonType="RAISED" prefHeight="28.0" prefWidth="208.0" ripplerFill="#00ff90" style="-fx-background-color: #23272d;" text="Upload" textFill="WHITE">
                     <font>
                        <Font name="Futura Bold" size="13.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="14.0" />
                     </VBox.margin>
                  </JFXButton>
               </children>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="20.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
